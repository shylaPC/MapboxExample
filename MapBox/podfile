platform :ios, '8.0'
use_frameworks!

def shared_pods
  pod 'NBNRequestKit', :git => 'https://github.com/1ec5/RequestKit.git', :branch => 'mapbox-podspec'
 pod 'MapboxGeocoder.swift', :git => 'https://github.com/mapbox/MapboxGeocoder.swift.git', :branch => 'master'

end

target 'MapBox' do
  shared_pods
end


